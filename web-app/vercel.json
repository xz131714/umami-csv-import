{
  "version": 2,
  "builds": [
    {
      "src": "app_lite.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app_lite.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "app_lite.py": {
      "maxDuration": 30
    }
  }
}